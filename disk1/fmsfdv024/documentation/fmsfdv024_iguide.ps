/DNF {false  DefineCMFont} def  % signal that the font is not already loaded
/DefineCMFont 
 {/loaded exch def  /typ exch def  /psz exch def  /ext exch def  /int exch def
  typ 10 ne
    {% font_type = 11, 12, 13
     loaded not  {/fnam ext 3 strip def  fnam findfont copydict 
      /newdict exch def  typ DefineCMEncoding  newdict 
      /Encoding CMEncoding put  ext newdict definefont pop} if
     int-dict-name ext findfont psz scalefont def
     currentdict int [int-dict /setfont cvx] cvx put
    }
    {% font_type = 10
     /fnam ext def  int-dict-name fnam findfont psz scalefont def
     currentdict int [int-dict /setfont cvx] cvx put
    }
  ifelse
 } def 
