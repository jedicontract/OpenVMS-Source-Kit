/DEC_fontcat_dict dup 100 dict def
load begin

/CaptionFont /Times-Roman findfont def		% Font for text about font.
/Tiny CaptionFont 6 scalefont def		% Font for tiny names, sizes..
/CatSize 24 def					% Size of big characters
/Temp 64 string def
/Base 8 def					% char code output base

/cell-x 36 def					% Wavelength of cells across
/cell-y -48 def					% Wavelength of cells down
/encoded-x 18 def				% X origin for encoded chars
/unencoded-x 18 def				% X origin for unencoded chars
/low-half-y 660 def				% Y origin for first table
/high-half-y 320 def				% Y origin for second table
/low-sample-y 376 def
/high-sample-y 36 def

/sample-size 10 def				% Font size of sample string
/tiny-size 6 def				% Font size of tiny text
/tiny-leading 8 def				% Leading for tiny text

/hang-left 3 def				% Amounts to hang char box lines
/hang-right 3 def
/hang-down 6 def

%%

/num-base hang-down tiny-size add neg def
/name-base num-base tiny-leading sub def
/width-x Tiny setfont (0000) stringwidth pop def

% Sort an array

/$shell 6 dict def
/shellsort {					% ary `shellsort' ary
    $shell begin
	/a exch def
	/n a length def
	/gap n 2 idiv def		% for (gap = n/2; gap > 0; gap /= 2)
	{   gap 0 le { exit } if

	    gap  1  n 1 sub		% for (i = gap; i < n; i++)
	    {	/i exch def

		i gap sub  gap neg  0
		{   /j exch def

		    a j get
		    a j gap add get
		    le { exit } if

		    a j get
		    a j gap add get
		    a j 3 -1 roll put
		    a j gap add 3 -1 roll put
		    }
		for
		}
	    for

	    /gap gap 2 idiv def
	    }
	loop
	a					% Return the array
	end
    } bind def

%
% name ccode printcc? `DoChar' ok?
% 
% Do single character of page. If printcc is false, then no character code is
% printed. Output to rectangle ll=(0,-24) ur=(36,24). The return value is a
% boolean indicating if the character is ok (otherwise, some error occurred).
% 

/DoChar {
    /printC? exch def
    /C exch def					% The character code
    /N exch def					% The name of the character
    /Str (_) dup 0 C put def			% A string with the char.
    /Err? false def				% Did an error occur?

    % Get the width of the character. An error may occur.

    /W F setfont Str stringwidth pop def
    % print the character name

    Tiny setfont
    N /.notdef ne
    {	0 name-base moveto
	N Temp cvs show
	}
    if

    % Print the character code or an em-dash.

    0 num-base moveto
    printC?
    {	C Base Temp cvrs show }			% Otheriwse, the code.
    {	(\320) show }				% An em-dash if no encoding
    ifelse

    % Print the character width, if we could compute it.

    width-x num-base moveto
    Err? 
    {	(???) show }
    {	W 0.0005 add Temp cvs 0 5 getinterval show }
    ifelse
 
    % print char with reference lines

    N /.notdef ne {
	hang-left 0 translate
	0 0 moveto

	FBig setfont Str show
	Err?
	{   Tiny setfont
	    Err Temp cvs show }
	{   /W W CatSize mul def		% Get the width for this size

	    0 hang-down neg moveto
	    0 CatSize lineto			% Left side of box
	    W hang-down neg moveto
	    W CatSize lineto			% Right side of box
	    hang-left neg 0 moveto
	    W hang-right add 0 lineto		% Base line
	    0 setlinewidth stroke
	    }
	ifelse
	}
    if

    Err? not
    } bind def
 
% 
% string `DoFontHeader' -
% 
% Prints the info at the top of a sheet. The string is printed after the font
% name as an indication of what lies on this sheet.
% 

/DoFontHeader {
    CaptionFont 18 scalefont setfont
    18 756 moveto FName Temp cvs show		% ( (24 point)) show
    CaptionFont 10 scalefont setfont
    show					% Picks up the arg string

    F /FontInfo known
    {	/Finfo F /FontInfo get def }
    {	/Finfo 1 dict def }
    ifelse

    400 740 moveto statusdict /product get show
    (, PostScript v) show version show
    ( r) show statusdict /revision get 10 string cvs show

    400 728 moveto (Font version: ) show
    Finfo /version known
    {	Finfo /version get show }
    {	((none)) show }
    ifelse
    18 740 moveto

    [ /FullName /FamilyName /Weight /Notice ]
    {	/key exch def
	Finfo key known
	{   key Temp cvs show (: ) show
	    Finfo key get show
	    currentpoint 12 sub exch pop 18 exch
	    moveto
	    }
	if
	}
    forall
    } bind def

% 
% fontname `DoFont' -
% 
% Print a font sample page.
% 

/DoFont {
    /FontLevel save def
    /FName exch def				% font name
    /F FName findfont def
    /FBig F CatSize scalefont def
    /Line0 96 string def
    /Line1 96 string def
    /l0 0 def
    /l1 0 def

    /has-cs? F /CharStrings known def

    has-cs?
    {	/charstrings F /CharStrings get def
	/names charstrings maxlength dict def
	/newchars 200 string def
	}
    if
 
    (  (encoded characters)) DoFontHeader

    0 1 95
    {	/I exch def
	/Xn I 16 mod def
	/Yn I 16 idiv def
 
	% chars 20-7F

	gsave
	    /ccode I 16#20 add def
	    /cname F /Encoding get ccode get def

	    has-cs?
	    {	names cname ccode put }		% Remember we found this one
	    if

	    Xn cell-x mul encoded-x add
	    Yn cell-y mul low-half-y add translate
	    cname ccode true DoChar
	    {	Line0 l0 ccode put
		/l0 l0 1 add def
		}
	    if

	    grestore

	% chars A0-FF

	gsave
	    /ccode I 16#A0 add def
	    /cname F /Encoding get ccode get def

	    has-cs?
	    {	names cname ccode put }
	    if

	    Xn cell-x mul encoded-x add
	    Yn cell-y mul high-half-y add translate
	    cname ccode true DoChar
	    {	Line1 l1 ccode put
		/l1 l1 1 add def
		}
	    if

	    grestore
	}
    for

    F sample-size scalefont setfont
    encoded-x low-sample-y moveto Line0 0 l0 getinterval show
    encoded-x high-sample-y moveto Line1 0 l1 getinterval show

    %showpage  ! DJS 13-Mar-1996 Causes a blank page to be printed at
    %          !                 the beginning of a document

    has-cs?
    {
    % Do unencoded characters

    (  (unencoded characters)) DoFontHeader

    names /.notdef -1 put
    /unencoded
    [	charstrings
	{   pop /cname exch def
	    names cname known not
	    {	% An unencoded character
		cname dup length string cvs }
	    if }
	forall
	] def					% We have an array of names.

    % Build a new encoding vector with the unencoded characters.

    F /Encoding get length array
    /newencoding exch def
    F /Encoding get aload pop newencoding astore pop
    /emptypos 0 def
    /newnum 0 def

    unencoded shellsort
    {	/cname exch cvn def

	{   newencoding emptypos get /.notdef eq
	    {	exit }
	    {	/emptypos emptypos 1 add store }
	    ifelse
	    }
	loop

	newencoding emptypos cname put
	newchars newnum emptypos put
	/newnum newnum 1 add store
	}
    forall

    % Make a new font with this encoding vector

    F length 1 add dict
    /newfont exch def
    F
    {	1 index /FID ne
	{   newfont 3 1 roll put }
	{   pop pop }
	ifelse }
    forall

    newfont /Encoding newencoding put
    /NewFont newfont definefont
    /F exch def
    /FBig F CatSize scalefont def

    /l0 0 def

    0 1 newnum 1 sub
    {	/I exch def
	/Xn I 16 mod def
	/Yn I 16 idiv def
	
	gsave
	    /ccode newchars I get def
	    /cname newencoding ccode get def

	    Xn cell-x mul unencoded-x add
	    Yn cell-y mul low-half-y add translate
	    cname ccode false DoChar
	    {	Line0 l0 ccode put
		/l0 l0 1 add def
		}
	    if
	    grestore
	}
    for

    F sample-size scalefont setfont
    unencoded-x low-sample-y moveto Line0 0 newnum getinterval show

    showpage

    }
    if	% has-cs?
    
    FontLevel restore
    } bind def
 
% Do a complete catalog

/DoAllFonts {
    [   FontDirectory
	{   pop dup length string cvs
	    }
	forall
	]
    shellsort
    {	cvn DoFont }
    forall
    } bind def

